<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mainFragment5">
    <fragment
        android:id="@+id/mainFragment5"
        android:name="com.example.projecttest2.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment5_to_iphoneFragment"
            app:destination="@id/iphoneFragment" />
        <action
            android:id="@+id/action_mainFragment5_to_promotionFragment"
            app:destination="@id/promotionFragment" />
        <action
            android:id="@+id/action_mainFragment5_to_videoFragment"
            app:destination="@id/videoFragment" />
    </fragment>
    <fragment
        android:id="@+id/iphoneFragment"
        android:name="com.example.projecttest2.IphoneFragment"
        android:label="fragment_iphone"
        tools:layout="@layout/fragment_iphone" />
    <fragment
        android:id="@+id/promotionFragment"
        android:name="com.example.projecttest2.PromotionFragment"
        android:label="fragment_promotion"
        tools:layout="@layout/fragment_promotion" />
    <fragment
        android:id="@+id/videoFragment"
        android:name="com.example.projecttest2.VideoFragment"
        android:label="fragment_video"
        tools:layout="@layout/fragment_video" />
</navigation>